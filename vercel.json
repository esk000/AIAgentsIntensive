{
  "rewrites": [
    {
      "source": "/public/(.*)",
      "destination": "/public/$1"
    },
    {
      "source": "/automated-grader",
      "destination": "/public/automated-grader.html"
    },
    {
      "source": "/.well-known/agent-card.json",
      "destination": "/.well-known/agent-card.json"
    },
    {
      "source": "/(.*)",
      "destination": "https://vendor-catalog-backend.example.com/$1"
    }
  ]
}